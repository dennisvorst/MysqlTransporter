{
  "name": "vorstd/transporter",
  "description": "Mysql data transporter.",
  "license": "proprietary",
  "autoload": {
    "classmap": [ "src/" ],
    "psr-0": { "\\": "src/class/" }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dennisvorst/mysqldatabase/"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "require": {
    "vorstd/database": "dev-master"
  },
  "scripts": {
    "test": "phpunit --colors=always --bootstrap ./vendor/autoload.php ./tests/Unit"
  }
}